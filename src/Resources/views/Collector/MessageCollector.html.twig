{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% if 0 < collector.count %}
        {% set icon %}
            <img width="28" height="28" alt="Swarrot" src="data:image/png;base64," />
            <span class="sf-toolbar-status">{{ collector.count }}</span>
        {% endset %}
        {% set text %}
            <div class="sf-toolbar-info-piece">
                <b>Messages</b>
                <span>{{ collector.count }}</span>
            </div>
        {% endset %}
        {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
    {% endif %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img alt="Swarrot" src="data:image/png;base64," /></span>
    <strong>Swarrot</strong>
    <span class="count">
        <span>{{ collector.count }}</span>
    </span>
</span>
{% endblock %}

{% block panel %}
    <h2>Messages</h2>
    {% if collector.count %}
        <table>
            <thead>
                <tr>
                    <th>Body</th>
                    <th>Gate</th>
                    <th>Connection</th>
                    <th>Exchange</th>
                    <th>Date</th>
                </tr>
            </thead>
            <tbody>
                {% for message in collector.messages %}
                <tr>
                    <td>{{ message.message.body }}</td>
                    <td>{{ message.gate }}</td>
                    <td>{{ message.connection.name }}</td>
                    <td>{{ message.exchange }}</td>
                    <td>{{ message.published_at|date('Y/m/d \\a\\t H:i:s', false) }}</td>
                </tr>
                {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>
            <em>No messages were sent.</em>
        </p>
    {% endif %}
{% endblock %}
